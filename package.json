{
  "name": "vue-daval",
  "version": "2.0.0",
  "description": "Vue data validator",
  "author": "ragnar lotus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deulos/vue-daval.git"
  },
  "keywords": [
    "vue",
    "data",
    "form",
    "model",
    "input",
    "validator",
    "validation"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deulos/vue-daval/issues"
  },
  "homepage": "https://github.com/deulos/vue-daval#readme",
  "main": "dist/vue-daval.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-docs": "vue-cli-service build --dest docs",
    "bundle": "vue-cli-service build --target lib --name vue-daval \"src/mixins/VueDaval.js\""
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "highlight.js": "^9.13.1",
    "vue": "^2.5.17",
    "vue-highlight.js": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "baseUrl": "/vue-daval/",
    "css": {
      "extract": false
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
