{
  "name": "vue-daval",
  "version": "2.0.7",
  "description": "Vue data validator",
  "author": "ragnar lotus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deulos/vue-daval.git"
  },
  "keywords": [
    "vue",
    "data",
    "form",
    "model",
    "input",
    "validator",
    "validation"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deulos/vue-daval/issues"
  },
  "homepage": "https://github.com/deulos/vue-daval#readme",
  "main": "dist/vue-daval.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-docs": "vue-cli-service build --dest docs",
    "bundle": "vue-cli-service build --target lib --name vue-daval \"src/mixins/VueDaval.js\"",
    "test:unit": "vue-cli-service test:unit --coverage --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "^2.5.20"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "highlight.js": "^9.13.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.1",
    "vue-highlight.js": "^2.2.0",
    "vue-template-compiler": "^2.5.20"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "baseUrl": "/vue-daval/",
    "css": {
      "extract": false
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
